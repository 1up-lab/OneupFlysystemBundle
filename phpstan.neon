parameters:
    bootstrapFiles:
        - %rootDir%/../../../vendor/autoload.php

    excludes_analyse:
        - src/DependencyInjection/Configuration.php
        - tests/App/cache
        - tests/App/logs
        - tests/App/var
        - var
        - vendor

    checkGenericClassInNonGenericObjectType: false
    checkMissingIterableValueType: false
    reportUnmatchedIgnoredErrors: false
