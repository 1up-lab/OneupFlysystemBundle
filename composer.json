{
    "name": "oneup/flysystem-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Flysystem filesystem abstraction library to your Symfony2 project.",
    "keywords": ["symfony2", "flysystem", "filesystem", "abstraction"],
    "homepage": "http://1up.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Schmid",
            "email": "js@1up.io",
            "homepage": "http://1up.io",
            "role": "Developer"
        }
    ],

    "require": {
        "symfony/framework-bundle": ">=2.0",
        "league/flysystem": "~1.0",
        "league/flysystem-rackspace": "1.0.0",
        "league/flysystem-webdav": "1.0.0",
        "league/flysystem-aws-s3-v3": "1.0.0",
        "league/flysystem-dropbox": "1.0.0",
        "league/flysystem-cached-adapter": "1.0.0",
        "league/flysystem-sftp": "1.0.0",
        "league/flysystem-ziparchive": "1.0.0"
    },

    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "symfony/finder": ">=2.0",
        "symfony/browser-kit": ">=2.0"
    },

    "suggest": {
    },

    "autoload": {
        "psr-0": { "Oneup\\FlysystemBundle": "" }
    },

    "target-dir": "Oneup/FlysystemBundle"
}
